---
layout: compress
---
[
  {%for trail in site.data.ovrdc_trails%}
  {% if trail.Status == "Future" %}
  {% elsif trail.subtype == "Extension" %}
  {% else %}
  {
    "trail":"{{trail.Name}}",
    "type": "{{trail.maptype}}",
    "url": "/trails/{{trail.Name | slugify}}/",
    "cities": "{{trail.cities}}",
    "counties": "{{trail.counties}}",
    "subtitle":"{% if trail.subtitle %}{{trail.subtitle}}{%else%} {%endif%}"
  },
  {% endif %}
  {%endfor%}
  {}
]
